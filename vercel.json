{
  "version": 2,
  "builds": [
    { 
      "src": "src/api/index.ts", 
      "use": "@vercel/node" 
    },
    { 
      "src": "front/package.json", 
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "src/api/index.ts" 
    },
    { 
      "src": "/(.*)", 
      "dest": "front/build/$1" 
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/front/$1" }
  ]
}








